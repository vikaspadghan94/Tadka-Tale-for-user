{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }



  {
    "version": 2,
    "builds": [
     {
         "src": "./server.js",
         "use": "@vercel/node"
     }
    ],
    "routes": [
     {
         "src": "/(.*)",
         "dest": "/"
     }
    ]
 }